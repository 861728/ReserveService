        "# âœ… STEP 6 ì™„ì „ êµì²´ - place ID ì§ì ‘ ì¶”ì¶œ ë°©ì‹\n",
        "import re, time, urllib.parse\n",
        "from selenium.webdriver.common.by import By\n",
        "from selenium.webdriver.support.ui import WebDriverWait\n",
        "from selenium.webdriver.support import expected_conditions as EC\n",
        "\n",
        "def get_phone_from_naver(place_name, driver):\n",
        "    try:\n",
        "        encoded = urllib.parse.quote(place_name)\n",
        "        driver.get(f'https://map.naver.com/p/search/{encoded}')\n",
        "        time.sleep(4)\n",
        "\n",
        "        wait = WebDriverWait(driver, 10)\n",
        "\n",
        "        # searchIframe ì§„ì…\n",
        "        iframe = wait.until(EC.presence_of_element_located((By.CSS_SELECTOR, 'iframe#searchIframe')))\n",
        "        driver.switch_to.frame(iframe)\n",
        "        time.sleep(2)\n",
        "\n",
        "        # ì²« ë²ˆì§¸ ê°€ê²Œ ë§í¬ì—ì„œ place ID ì¶”ì¶œ\n",
        "        place_id = None\n",
        "        links = driver.find_elements(By.CSS_SELECTOR, 'a')\n",
        "        for a in links:\n",
        "            href = a.get_attribute('href') or ''\n",
        "            # entryIframe srcì—ì„œ place ID ì¶”ì¶œ ì‹œë„\n",
        "            match = re.search(r'/place/(\\d+)', href)\n",
        "            if match:\n",
        "                place_id = match.group(1)\n",
        "                break\n",
        "\n",
        "        driver.switch_to.default_content()\n",
        "\n",
        "        # place IDë¡œ ì§ì ‘ entryIframe URL ì ‘ê·¼\n",
        "        if not place_id:\n",
        "            # place ID ì—†ìœ¼ë©´ pcmap ì§ì ‘ ì ‘ê·¼\n",
        "            driver.get(f'https://pcmap.place.naver.com/place/list?query={encoded}')\n",
        "            time.sleep(3)\n",
        "            src = driver.page_source\n",
        "            match = re.search(r'\"id\":\"(\\d{6,})\"', src)\n",
        "            if match:\n",
        "                place_id = match.group(1)\n",
        "\n",
        "        if not place_id:\n",
        "            return ''\n",
        "\n",
        "        # place ìƒì„¸ í˜ì´ì§€ ì§ì ‘ ì ‘ê·¼\n",
        "        driver.get(f'https://pcmap.place.naver.com/restaurant/{place_id}/home')\n",
        "        time.sleep(3)\n",
        "\n",
        "        # ì „í™”ë²ˆí˜¸ ì¶”ì¶œ\n",
        "        src = driver.page_source\n",
        "\n",
        "        # 1ìˆœìœ„: tel: ë§í¬\n",
        "        tel_match = re.search(r'href=\"tel:([^\"]+)\"', src)\n",
        "        if tel_match:\n",
        "            return tel_match.group(1).strip()\n",
        "\n",
        "        # 2ìˆœìœ„: ì „í™”ë²ˆí˜¸ íŒ¨í„´\n",
        "        matches = re.findall(r'0\\d{2,3}-\\d{3,4}-\\d{4}', src)\n",
        "        if matches:\n",
        "            return matches[0]\n",
        "\n",
        "        return ''\n",
        "\n",
        "    except Exception as e:\n",
        "        return ''\n",
        "    finally:\n",
        "        driver.switch_to.default_content()\n",
        "\n",
        "print('âœ… í•¨ìˆ˜ êµì²´ ì™„ë£Œ!')"
